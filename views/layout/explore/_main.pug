              // end::Header
              // begin::License
              .rounded.border.border-dashed.border-gray-300.py-4.px-6.mb-5
                .d-flex.flex-stack
                  .d-flex.flex-column
                    .d-flex.align-items-center.mb-1
                      .fs-6.fw-bold.text-gray-800.fw-bold.mb-0.me-1 Regular License
